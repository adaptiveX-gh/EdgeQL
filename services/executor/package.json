    "@types/uuid": "^9.0.0",
    "@types/dockerode": "^3.3.0",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0",
    "tsc-alias": "^1.8.0",
    "eslint": "^8.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "vitest": "^1.0.0"
  }
